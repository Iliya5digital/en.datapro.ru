(function(a){"function"===typeof define&&define.amd?define(["jquery","hammerjs"],a):"object"===typeof exports?a(require("jquery"),require("hammerjs")):a(jQuery,Hammer)})(function(a,c){a.fn.hammer=function(e){return this.each(function(){var b=a(this);b.data("hammer")||b.data("hammer",new c(b[0],e))})};c.Manager.prototype.emit=function(c){return function(b,d){c.call(this,b,d);a(this.element).trigger({type:b,gesture:d})}}(c.Manager.prototype.emit)});;
function eventsController(options){options=$.extend({max_num:5,min_num:0,num:0,cycle:false,changeEvent:function changeEvent(){},tryChangeEvent:function tryChangeEvent(){},autoRemoveEvents:true,scroll_timeout:0,btns:false,dots:false,progress:false,auto:false,scrollEvents:true,keyboardEvents:true,swipeEvents:false},options);var $item=options.el;var listeners=[];var events_enabled=true;var scroll_av=true;var scroll_pos=0;var scroll_timeout,auto_timeout;var btns={};if(!options.num){options.num=0;}
function init(){if(options.scrollEvents){$item.on('mousewheel',function(event){if(scroll_av){scroll_event(-0.005*event.deltaY*event.deltaFactor);}});}
if(options.keyboardEvents){listeners.push(emitter.subscribe('keydown',function(key){if(key==39){scroll_event(1);}else if(key==37){scroll_event(-1);}}));}
if(options.swipeEvents){var events_increase,events_decrease,directions;if(options.swipeEvents=='h'){events_increase='swipeleft';events_decrease='swiperight';direction=Hammer.DIRECTION_HORIZONTAL;}else if(options.swipeEvents=='v'){events_increase='swipeup';events_decrease='swipedown';direction=Hammer.DIRECTION_VERTICAL;}else{events_increase='swipeleft swipeup';events_decrease='swiperight swipedown';direction=Hammer.DIRECTION_ALL;}
$item.on(events_increase,function(){scroll_event(1);}).on(events_decrease,function(){scroll_event(-1);}).hammer().data('hammer').get('swipe').set({direction:direction});}
if(options.btns){if(!options.btns.length){options.el.append('<div class="arrow left" data-targ="-1"></div><div class="arrow right" data-targ="1"></div>');options.btns=options.el.find('.arrow');}
btns.left=options.btns.filter('[data-targ="-1"]');btns.right=options.btns.filter('[data-targ="1"]');options.btns.on('click',function(){scroll_event($(this).data('targ'));});}
if(options.dots){var html='<div class="dots_frame">';for(var i=0;i<=options.max_num;i++){html+='<div class="dot" data-targ="'+i+'"></div>';}
html+='</div>';options[options.dots.length?'dots':'el'].append(html).on('click','.dot',function(){change_current($(this).data('targ')-options.num);});html=null;}
if(options.progress){var html='<div class="progress_frame"><div class="progress div_100"></div></div>';options[options.progress.length?'progress':'el'].append(html);}
if(options.autoRemoveEvents){listeners.push(emitter.subscribe('page.closed',function(){methods.removeEvents();}));}
options.changeEvent(options.num);updateControls();startAuto();}
function startAuto(){if(options.auto){clearTimeout(auto_timeout);auto_timeout=setTimeout(function(){change_current(1);},options.auto);}}
function stopAuto(){if(options.auto){clearTimeout(auto_timeout);}}
function get_img_num(delta){var r=options.num+delta;if(r>options.max_num){r=options.cycle?options.min_num-1+(r-options.max_num):options.max_num;}else if(r<options.min_num){r=options.cycle?options.max_num+1+(r-options.min_num):options.min_num;}
return r;}
