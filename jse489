function mouseTrail(options){options=$.extend({interval:16,decay:0.08},options);var positions={cur:{x:0,y:0},prev:{x:0,y:0}};var moveInterval;function calc_position(){positions.cur.x+=(mouse_pos.pageX / frame_w-positions.cur.x)*options.decay;positions.cur.y+=(mouse_pos.pageY / frame_h-positions.cur.y)*options.decay;if(positions.cur.x!==positions.prev.x&&positions.cur.y!==positions.prev.y){emitter.emit('mousetrail',[positions.cur.x,positions.cur.y,positions.prev.x,positions.prev.y]);positions.prev.x=positions.cur.x;positions.prev.y=positions.cur.y;}}
function init(){if(!mouse_pos){mouse_pos={pageX:frame_w*0.5,pageY:frame_h*0.5};}
