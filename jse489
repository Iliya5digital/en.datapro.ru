function native_scroll_controller(options2){textScroll__content.on('scroll',function(){var pos_px=textScroll__content.scrollTop();var pos=pos_px / Math.floor(heights_delta);options2.onMove(pos,pos_px);});this.move=function(options3){textScroll__content.stop(true).animate({'scrollTop':options3.target*heights_delta},options3.time==undefined?options2.animation_time:options3.time,'easeInOutQuad',function(){if(options3.callback){options3.callback();}});};this.setPosition=function(pos){textScroll__content.stop(true).scrollTop(pos*heights_delta);};this.setScrollSpeed=function(){};this.removeEvents=function(){textScroll__content.off();};return this;}
function setMode(){if(scroll_controller){textScroll__content.removeAttr('style');scroll_controller.removeEvents();}
if(options["native"]){if(textScroll__size.data('hammer')){textScroll__size.data('hammer').destroy();textScroll__size.data('hammer',null).removeAttr('style');}
$item.addClass('native');textScroll__content.css({'right':textScroll__size.width()-$item.width()});scroll_controller=new native_scroll_controller({animation_time:options.animateTime,onMove:function onMove(pos,pos_px){setScrollPosition(pos,pos_px);}});}else{$item.removeClass('native');scroll_controller=new scrollController({events_area:options.eventsArea||$item,animation_time:options.animateTime,scroll_time:options.scrollTime,start_position:options.start_position,keyboard_events:true,autoRemoveEvents:options.autoRemoveEvents,onMove:function onMove(pos){var pos_px=Math.floor(pos*heights_delta);setScrollPosition(pos,pos_px);},animation_callback_before:function animation_callback_before(){pan_data.animated=true;},animation_callback_after:function animation_callback_after(){pan_data.animated=false;},tryChangeEvent:function tryChangeEvent(delta){options.tryChangeEvent(delta);}});}
_resize();}
function setScrollPosition(pos,pos_x){scroll_position=Math.max(0,Math.min(pos,1));scroll_position_px=pos_x;textScroll__track_move.css(get_transition_css(scroll_position*100+'%'));if(!options["native"]){textScroll__content.css(get_transition_css(-scroll_position*100*(max_scroll_position-1)+'%'));}
setTimeout(function(){options.onMove(scroll_position,scroll_position_px);},0);}
function _resize(params){params=$.extend({preserve_position:false},params);textScroll__size_height=textScroll__size.outerHeight();textScroll__content_height=textScroll__content.height();heights_delta=textScroll__size_height-textScroll__content_height;max_scroll_position=Math.max(1,textScroll__size_height / textScroll__content_height);if(max_scroll_position==1){$item.addClass(_this.title+'--no_scroll');}else{$item.removeClass(_this.title+'--no_scroll');}
if(params.fix_position){if(options["native"]){setScrollPosition(textScroll__content.scrollTop()/ heights_delta);}else{scroll_controller.setPosition(-textScroll__content.position().top / heights_delta);}}else if(!params.preserve_position){scroll_controller.setPosition(scroll_position);}
var scroll_speed=options.speed*2 /((max_scroll_position-1)*textScroll__content_height);if(scroll_speed==Infinity){scroll_speed=0;}
scroll_controller.setScrollSpeed(scroll_speed);options.onReinitialise();}
function _scrollToScreen(delta,time){var new_pos=0.01*(scroll_position*100+delta*100 /(max_scroll_position-1));scroll_controller.move({target:new_pos,time:time});}
function stop_animation(){if(pan_data.animated){pan_data.animated=false;scroll_controller.stop();}}
function dragInit(element,parent,text_drag){if(!element.data('hammer')){var calc_controller_position=function calc_controller_position(pos,normalize){pos /=pan_data.drag_height*(1-(text_drag?max_scroll_position:0));if(normalize){pos=Math.max(0,Math.min(1,pos));}
return pos;};element.hammer();element.data('hammer').get('pan').set({direction:Hammer.DIRECTION_VERTICAL,threshold:0});element.data('hammer').get('press').set({time:0});element.on('press panstart panmove panend',function(e){if(events_on&&(!text_drag||options.dragEvents)){if(e.type=='press'){stop_animation();}else if(e.type=='panstart'){stop_animation();pan_data.drag_start_y=parent.position().top;pan_data.drag_height=parent.height();}else if(e.type=='panmove'){var pos_y=calc_controller_position(pan_data.drag_start_y+e.gesture.deltaY,true);scroll_controller.setPosition(pos_y);}else if(e.type=='panend'){var time=Math.abs(400*e.gesture.velocityY);var dist=0.5*e.gesture.velocityY*time;var pos_y=calc_controller_position(pan_data.drag_start_y+e.gesture.deltaY+dist,false);scroll_controller.move({target:pos_y,time:time,easing:'quadEaseOut'});}}});}}
function setAutoresize(){clearInterval(autoresizeInterval);if(options.autoresize){autoresizeInterval=setInterval(function(){_resize();},options.autoresize);}
_resize();}
function setImgresize(){textScroll__size.find('img').off('load.text_scroll').on('load.text_scroll',function(){_resize();});}
var methods={resize:function resize(params){_resize(params);},removeEvents:function removeEvents(){scroll_controller.removeEvents();clearInterval(autoresizeInterval);textScroll__drag.off();textScroll__size.off();textScroll__track.off();textScroll__track.find('.textScroll__page_btn').off();$item.off();listeners.forEach(function(listener){listener.unsubscribe();});},scrollTo:function scrollTo(position,time,callback){scroll_controller.move({target:position,time:time,callback:callback});},setPosition:function setPosition(position){scroll_controller.setPosition(position);},scrollToScreen:function scrollToScreen(delta,time){_scrollToScreen(delta,time);},scrollToElement:function scrollToElement(selector,time,callback,target){var element=$item.find(selector);if(element.length){var target_delta=0;if(target=='bottom'){target_delta=element.outerHeight()-textScroll__content_height;}else if(target=='center'){target_delta=0.5*(element.outerHeight()-textScroll__content_height);}
var position=(element.offset().top-textScroll__size.offset().top+target_delta)/ heights_delta;scroll_controller.move({target:position,time:time,callback:callback});}},getScrollPosition:function getScrollPosition(){return[scroll_position,scroll_position_px];},loadHtml:function loadHtml(html,time,callback){var time=time||0;$item.transitionStop(true).transition({'opacity':0},time,function(){textScroll__size.find('.content').html(html);setImgresize();_resize();scroll_controller.setPosition(0);$item.transition({'opacity':1},time);if(callback){callback();}});},eventsOn:function eventsOn(){events_on=true;scroll_controller.eventsOn();$item.removeClass(_this.title+'--events_off');},eventsOff:function eventsOff(){events_on=false;scroll_controller.eventsOff();$item.addClass(_this.title+'--events_off');},setOptions:function setOptions(new_options){$.extend(options,new_options);if(new_options.dragEvents==true&&!options["native"]){dragInit(textScroll__size,textScroll__content,true);}
if(new_options.autoresize!==undefined){setAutoresize();}
if(new_options["native"]!==undefined){setMode();}}};$item.data(_this.title,methods);init();}});;
pluginFactory({title:'sequenceSlider',options:{path:'/assets/images/fly/',filename:null,start_num:0,min_num:0,max_num:false,show_points:false,time_auto:30,short_path:true,sensitivity:30,off:false,repeat:true,noBigImg:false,keyframes:null,scroll_direction:-1,scroll_disable:false,drag_disable:false,changeImg:function changeImg(){},showContent:function showContent(num,el){el.transitionStop(true).css({'display':'block','opacity':0}).transition({'opacity':1},200);},hideContent:function hideContent(num,el){el.transitionStop(true).transition({'opacity':0},100,function(){$(this).css({'display':'none'});});},showContentEvent:function showContentEvent(){},hideContentEvent:function hideContentEvent(){},loadComplete:function loadComplete(){},changeBigImg:function changeBigImg(){},afterBigImg:function afterBigImg(){},tryChangeEvent:function tryChangeEvent(){},autoRemoveEvents:true},code:function code($item,options){var _this=this;if(!options.events_frame){options.events_frame=$item;}
var max_num;var cur_num=options.start_num;var scroll_pos=0;var events_on=false;var move_start_num,move_next_num,scroll_interval,scroll_targ,load_img_big_timeout,scroll_callback,clear_prev_img;var frames=$item.add(options.events_frame);var previews;var listeners=[];function init(){frames.addClass(_this.title);if(options.filename){options.filename=options.path+options.filename;}else{options.filename=options.path+'tmb/preview'+(!mobile?'':'_mobile')+'.txt';}
$.ajax({url:options.filename,cache:true,type:'GET',success:function success(data){previews=data.split('\n').slice(0,-1);max_num=options.min_num+previews.length-1;previewsLoaded();}});if(options.autoRemoveEvents){listeners.push(emitter.subscribe('page.closed',function(position){methods.removeEvents();}));}}
function previewsLoaded(){options.loadComplete();if(!options.off){events_on=true;}
if(!options.scroll_disable){options.events_frame.on('mousewheel.sequenceSlider',function(event){if(events_on){var delta=0.01*event.deltaY*event.deltaFactor;scroll_pos+=options.scroll_direction*delta;if(Math.abs(scroll_pos)>0.51){clear_timeouts();if(!options.max_num||options.max_num>=cur_num+Math.round(scroll_pos)){move_next_num=cur_num+Math.round(scroll_pos);load_img({load_big_img:true});}
scroll_pos=0;}}});}
if(!options.drag_disable){options.events_frame.hammer().css({'cursor':'url("/assets/i/cur1.cur"), move'}).on('panstart.sequenceSlider',function(){if(events_on){$(this).css({'cursor':'url("/assets/i/cur2.cur"), move'});move_start_num=cur_num;clear_timeouts();}}).on('panmove.sequenceSlider',function(e){if(events_on){move_next_num=move_start_num-Math.floor(e.gesture.deltaX / options.sensitivity);load_img();}}).on('panend.sequenceSlider pancancel.sequenceSlider',function(){if(events_on){$(this).css({'cursor':'url("/assets/i/cur1.cur"), move'});load_img_big();}});}
show_content();load_img_big(0);options.changeImg(cur_num);}
function clear_timeouts(){scroll_targ=null;clearInterval(scroll_interval);clearTimeout(load_img_big_timeout);}
function show_content(){var show_content=frames.find('.fly_content.n'+cur_num);if(show_content.length&&!show_content.hasClass('active')){show_content.addClass('active');options.showContent(cur_num,show_content);}
options.showContentEvent(cur_num);}
function load_img(opt){opt=$.extend({forced:false,load_big_img:false},opt);if(move_next_num<options.min_num){if(options.repeat){move_next_num=max_num-options.min_num+(move_next_num+1)%(max_num-options.min_num+1);}else{move_next_num=options.min_num;options.tryChangeEvent(-1);}}else if(move_next_num>max_num){if(options.repeat){move_next_num=move_next_num%(max_num-options.min_num+1);}else{move_next_num=max_num;options.tryChangeEvent(1);}}
if(move_next_num!=cur_num||opt.forced){var hide_content=frames.find('.fly_content.active');if(hide_content.length){hide_content.removeClass('active');options.hideContent(cur_num,hide_content);options.hideContentEvent(cur_num);}
cur_num=move_next_num;$item.find('.sequence_img:last').attr('src',previews[cur_num-options.min_num]);if(scroll_targ==cur_num||options.show_points&&!scroll_targ){show_content();}
if(scroll_targ==cur_num||opt.force_load_big){clear_timeouts();load_img_big();if(scroll_callback){scroll_callback();scroll_callback=null;}}
options.changeImg(cur_num);}
if(opt.load_big_img){load_img_big();}}
function load_img_big(time){clear_timeouts();if(time==null){time=300;}
load_img_big_timeout=setTimeout(function(){if(!options.noBigImg){options.changeBigImg(cur_num);$item.find('.sequence_img:last').after('<img class="div_100 video-bg sequence_img" src="'+options.path+(!mobile?'':'m/')+cur_num+'.jpg" />').end().find('.sequence_img:last').css({'translate3d':0}).on('load',function(){var last_img=$(this);last_img.off('load');clear_prev_img=setTimeout(function(){last_img.prevAll('.sequence_img').remove();options.afterBigImg();},100);});}
if(options.keyframes){if($.inArray(cur_num,options.keyframes)==-1){var target_frame;var prev_closest_frame=getClosestKeyframe(-1);var next_closest_frame=getClosestKeyframe(1);var prev_delta=cur_num-prev_closest_frame;if(prev_delta<0){prev_delta+=max_num;}
var next_delta=next_closest_frame-cur_num;if(next_delta<0){next_delta+=max_num+1;}
if(prev_delta<next_delta){target_frame=prev_closest_frame;}else{target_frame=next_closest_frame;}
methods.moveToNumber(target_frame);}}},time);}
function getClosestKeyframe(direction){var target_frame;if(direction==-1){target_frame=options.keyframes[options.keyframes.length-1];$.each(options.keyframes,function(index,value){if(value<cur_num){target_frame=value;}});}else{target_frame=options.keyframes[0];$.each(options.keyframes,function(index,value){if(value>cur_num){target_frame=value;return false;}});}
return target_frame;}
var methods={moveToDirection:function moveToDirection(dir){methods.moveToNumber(getClosestKeyframe(Number(dir)));},moveToNumber:function moveToNumber(targ_num,callback,direction){targ_num=Number(targ_num);if(callback){scroll_callback=callback;}
clear_timeouts();scroll_targ=targ_num;if(targ_num==cur_num){move_next_num=targ_num;load_img({forced:true});}else{if(!direction){var direction=-1;if(options.short_path){var drag_half=0.5*(max_num-options.min_num);if(targ_num>cur_num&&targ_num-cur_num<drag_half||targ_num<cur_num&&targ_num-options.min_num+max_num-cur_num<drag_half){direction=1;}}else{if(targ_num>cur_num){direction=1;}}}
scroll_interval=setInterval(function(){move_next_num=cur_num+direction;load_img();},options.time_auto);}},goToNumber:function goToNumber(targ_num){clear_timeouts();move_next_num=targ_num;load_img({load_big_img:true,force_load_big:true});},play:function play(callback){methods.moveToNumber(max_num,callback,1);},stop:function stop(){clear_timeouts();},getCurrent:function getCurrent(){return cur_num;},eventsOn:function eventsOn(){events_on=true;},eventsOff:function eventsOff(){events_on=false;},getMax:function getMax(){return max_num;},removeEvents:function removeEvents(){clear_timeouts();options.events_frame.off('.sequenceSlider');listeners.forEach(function(listener){listener.unsubscribe();});}};$item.data(_this.title,methods);init();}});;
page_js.pages["other"] = function(page,frame,frame1,frame2){
page.unset=function(){frame.off();};text_appear(frame);var page__scroll=frame.find('.page__scroll').textScroll({onMove:function onMove(pos,pos_px){}});page__scroll.eventsOff();var events_controller;var total_over_pos=0,prev_over_pos=0;page.init=function(){events_controller=new eventsController({el:frame.find('.page__events'),keyboardEvents:false,num:0,min_num:0,max_num:0,tryChangeEvent:function tryChangeEvent(delta){var res=change_page(delta);if(res)events_controller.eventsOff();}});};frame.on('click','.details_btn',function(){events_controller.eventsOff();page__scroll.eventsOn();frame.find('.seq_fr').transition({'y':'-50%'},500);$body.addClass('landing-active landing-popup-opened');frame.find('.landing').addClass('active');}).on('click','.landing-close',function(){page__scroll.scrollTo(0,600,function(){frame.find('.seq_fr').transition({'y':'0%'},500);$body.removeClass('landing-active landing-popup-opened');frame.find('.landing').removeClass('active');});page__scroll.eventsOff();events_controller.eventsOn();}).on('click','.page__top',function(){page__scroll.scrollTo(0,600);});
};;
