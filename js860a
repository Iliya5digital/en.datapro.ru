function mapsInit(options){options=$.extend({center:[55.7529,37.6211],zoom:15,zoomControl:false,keyboard:false,zoomButtons:false},options);var r=getRandomId();options.container.attr('id',r).addClass('mapsInit');var map=L.map(r,options);if(options.zoomButtons){if(options.zoomButtons.length){function update_zoom_buttons(cur_zoom){options.zoomButtons.filter('.in').toggleClass('disabled',cur_zoom>=map.getMaxZoom()).end().filter('.out').toggleClass('disabled',cur_zoom<=map.getMinZoom());}
options.zoomButtons.on('click',function(){var cur_zoom=map.getZoom()+Number($(this).data('targ'));map.setZoom(cur_zoom);update_zoom_buttons(cur_zoom);});update_zoom_buttons(options.zoom);}else{L.control.zoom({position:'topright',zoomInTitle:'',zoomOutTitle:''}).addTo(map);}}
map.attributionControl.setPrefix('').addAttribution('<a href="http://www.2gis.ru" target="_blank" title="2    ">&copy; 2    </a>');L.tileLayer('https://tile{s}.maps.2gis.com/tiles?x={x}&y={y}&z={z}&ts='+(mobile?'online_hd':'online_sd'),{subdomains:['0','1','2','3','4']}).addTo(map);map.marker=function(opt){var marker=L.marker(opt.position,{icon:L.divIcon({html:opt.html,iconSize:[0,0],className:opt.className}),riseOnHover:true,keyboard:false,title:opt.title});if(opt.click){marker.on('click',function(e){options.click(marker,e);})}
if(!opt.noAdd){marker.addTo(map);}
return marker;}
map.cluster=function(){return L.markerClusterGroup({iconCreateFunction:function(cluster){var markers=cluster.getAllChildMarkers();var titles=[];markers.forEach(function(item){if(item.title){titles.push(item.title);}})
return L.divIcon({html:'<div class="maps-marker-cluster"><div class="icon" title="'+titles.join(', ')+'"><div class="title">'+markers.length+'</div></div></div>'});},showCoverageOnHover:false}).addTo(map);}
map.overlay=function(opt){opt=$.extend(true,{image:'/assets/i/map_overlay.png',position:[[55.732530,37.578751],[55.733132,37.580179]]},opt);return L.imageOverlay(opt.image,opt.position).addTo(map);}
return map;};
function scrollController(options){options=$.extend({decay:0.08,scroll_speed:0.02,magnet:false,magnet_delta:0,start_position:0,min_position:0,max_position:1,scroll_events:true,swipe_events:false,keyboard_events:false,keyboard_speed:300,scroll_time:500,magnet_time:1000,animation_time:1000,animation_callback_before:function animation_callback_before(){},animation_callback_after:function animation_callback_after(){},onMove:function onMove(){},tryChangeEvent:function tryChangeEvent(){},autoRemoveEvents:true},options);var scroll_speed;var scroll_pos=options.start_position;var scroll_av=true;var events_enabled=true;var scroll_active_deltas=options.start_position;var scroll_keyframe=scroll_pos;var old_scroll_pos=null;var r_animate_obj={};var quadEaseIn=makeEaseIn(quad);var quadEaseOut=makeEaseOut(quad);var quadEaseInOut=makeEaseInOut(quad);var requestId;var listeners=[];var animationCallback=false;function init(){methods.setScrollSpeed(options.scroll_speed);options.animation_callback_before(scroll_keyframe);options.animation_callback_after(scroll_keyframe);requestId=requestAnimationFrame(function change_position_view(){scroll_pos=scroll_pos+(scroll_active_deltas-scroll_pos)*(scroll_av?options.decay:1);scroll_pos=Math.max(options.min_position,Math.min(options.max_position,scroll_pos));if(Math.abs(scroll_active_deltas-scroll_pos)<0.0005)scroll_pos=scroll_active_deltas;if(scroll_pos!==old_scroll_pos){old_scroll_pos=scroll_pos;options.onMove(scroll_pos);}else{}
requestId=requestAnimationFrame(change_position_view);});if(options.scroll_events){options.events_area.on('mousewheel',function(event){if(scroll_av&&events_enabled){scroll_event(-scroll_speed*event.deltaY*event.deltaFactor);}});}
if(options.swipe_events){options.events_area.on('swipeleft swipeup',function(){if(events_enabled){screen_move({delta:1});}}).on('swiperight swipedown',function(){if(events_enabled){screen_move({delta:-1});}}).hammer().data('hammer').get('swipe').set({direction:Hammer.DIRECTION_ALL});}
if(options.keyboard_events){listeners.push(emitter.subscribe('keydown',function(key){if(key==40){scroll_event(options.keyboard_speed*scroll_speed);}else if(key==38){scroll_event(-options.keyboard_speed*scroll_speed);}}));}
if(options.autoRemoveEvents){listeners.push(emitter.subscribe('page.closed',function(position){methods.removeEvents();}));}}
function makeEaseIn(timing){return function(timeFraction){return timing(timeFraction);};}
function makeEaseOut(timing){return function(timeFraction){return 1-timing(1-timeFraction);};}
function makeEaseInOut(timing){return function(timeFraction){if(timeFraction<0.5){return timing(2*timeFraction)/ 2;}else{return(2-timing(2*(1-timeFraction)))/ 2;}};}
function quad(timeFraction){return Math.pow(timeFraction,2);}
function get_easing(type){if(type=='quadEaseIn'){return quadEaseIn;}else if(type=='quadEaseOut'){return quadEaseOut;}else{return quadEaseInOut;}}
function scroll_event(delta){scroll_active_deltas+=delta;if(scroll_active_deltas>options.max_position){options.tryChangeEvent(1);}else if(scroll_active_deltas<options.min_position){options.tryChangeEvent(-1);}
scroll_active_deltas=Math.min(options.max_position,Math.max(options.min_position,scroll_active_deltas));var target_keyframe=null;if(options.magnet){var new_scroll_pos=Math.max(options.min_position,Math.min(options.max_position,scroll_pos+scroll_active_deltas));var direction=1;if(delta<0){direction=-1;}
$.each(options.keyframes,function(i,v){if((new_scroll_pos-v)*direction>options.magnet_delta&&new_scroll_pos*direction<options.keyframes[i+direction]*direction-options.magnet_delta){target_keyframe=options.keyframes[i+direction];return false;}});}
if(target_keyframe!==null){scroll_active_deltas-=delta;screen_move({target:target_keyframe,time:options.magnet_time});}else if(scroll_pos!==new_scroll_pos){}else{scroll_active_deltas=0;scroll_active_deltas-=delta;}}
function screen_move(opt){if(opt.delta!=undefined){var target_keyframe_num;options.keyframes.forEach(function(item,i){if(scroll_pos>=item){target_keyframe_num=i;}});target_keyframe_num=Math.max(0,Math.min(options.keyframes.length-1,target_keyframe_num+Number(opt.delta)));opt.target=options.keyframes[target_keyframe_num];}
if(opt.target!==undefined){scroll_av=false;opt.target=Number(opt.target);scroll_active_deltas=scroll_pos;if(opt.time===undefined){opt.time=options.animation_time;}
var scroll_start=scroll_pos;var scroll_delta=opt.target-scroll_pos;animationCallback=opt.callback;new r_animate({duration:opt.time,draw:function draw(progress){scroll_active_deltas=Math.max(options.min_position,Math.min(options.max_position,scroll_start+scroll_delta*progress));},callback:function callback(){if(opt.callback){opt.callback(scroll_pos);}
options.animation_callback_after(scroll_pos);scroll_av=true;scroll_active_deltas=scroll_pos;},timing:get_easing(opt.easing),stop_other:true});scroll_keyframe=opt.target;options.animation_callback_before(scroll_keyframe);}}
function stop_animation(){Object.keys(r_animate_obj).forEach(function(key){r_animate_obj[key].play=false;});}
function r_animate(opt){if(opt.stop_other){stop_animation();}
var start=performance.now();var _this=this;_this.play=true;function del_this(){delete r_animate_obj[start];}
requestAnimationFrame(function r_animate(time){if(_this.play==true){time=performance.now();var timeFraction=(time-start)/ opt.duration;if(timeFraction>1)timeFraction=1;var progress=timeFraction;if(opt.timing){progress=opt.timing(timeFraction);}
opt.draw(progress);if(timeFraction<1){requestAnimationFrame(r_animate);}else{if(opt.callback){opt.callback();}
del_this();}}else{if(opt.callback){opt.callback();}
del_this();}});r_animate_obj[start]=_this;}
var methods={setScrollSpeed:function setScrollSpeed(val){scroll_speed=val*0.01;},move:function move(opt){screen_move(opt);},stop:function stop(){stop_animation();},removeEvents:function removeEvents(){options.events_area.off();listeners.forEach(function(listener){listener.unsubscribe();});},getPosition:function getPosition(){return scroll_pos;},setPosition:function setPosition(pos){old_scroll_pos=null;scroll_pos=pos;scroll_active_deltas=scroll_pos;},eventsOff:function eventsOff(){events_enabled=false;},eventsOn:function eventsOn(){events_enabled=true;}};$.extend(this,methods);init();return this;};
pluginFactory({title:'textScroll',options:{speed:50,pageButtons:false,animateTime:500,scrollTime:300,eventsArea:null,dragEvents:false,"native":false,autoResize:0,start_position:0,onMove:function onMove(){},autoRemoveEvents:true,onReinitialise:function onReinitialise(){},tryChangeEvent:function tryChangeEvent(){}},code:function code($item,options){var _this=this;var scroll_position=0;var scroll_position_px=0;var max_scroll_position;var pan_data={};var autoresizeInterval;var events_on=true;var textScroll__size_height,textScroll__content_height,scroll_controller,heights_delta;var textScroll__content,textScroll__size,textScroll__drag,textScroll__track,textScroll__track_size,textScroll__track_move;var listeners=[];function init(){$item.removeClass('browser_scroll').addClass(_this.title).append('<div class="clear"></div>').wrapInner('<div class="'+_this.title+'__frame div_100"><div class="'+_this.title+'__content div_100"><div class="'+_this.title+'__size"></div></div></div>').append('<div class="'+_this.title+'__track"><div class="'+_this.title+'__track_size"><div class="'+_this.title+'__track_move"><div class="'+_this.title+'__drag"></div></div></div></div>');textScroll__content=$item.find('.'+_this.title+'__content');textScroll__size=$item.find('.'+_this.title+'__size');textScroll__drag=$item.find('.'+_this.title+'__drag');textScroll__track=$item.find('.'+_this.title+'__track');textScroll__track_size=$item.find('.'+_this.title+'__track_size');textScroll__track_move=$item.find('.'+_this.title+'__track_move');if(options.pageButtons){textScroll__track.prepend('<div class="'+_this.title+'__page_btn top" data-targ="-1"></div><div class="'+_this.title+'__page_btn bottom" data-targ="1"></div>');textScroll__track.find('.'+_this.title+'__page_btn').on('click',function(){if(events_on){_scrollToScreen(Number($(this).data('targ')),options.animateTime);}});}
textScroll__track.on('click',function(e){if(e.target.className=='textScroll__track'&&events_on){var position=(e.offsetY-parseInt(textScroll__track_size.css('margin-top'))-0.5*textScroll__drag.height())/ textScroll__track_size.height();scroll_controller.move({target:position});}});setMode();setAutoresize();setImgresize();dragInit(textScroll__drag,textScroll__track_move,false);if(options.dragEvents&&!options["native"]){dragInit(textScroll__size,textScroll__content,true);}
listeners.push(emitter.subscribe('window.resize',function(){_resize();}));if(options.autoRemoveEvents){listeners.push(emitter.subscribe('page.closed',function(position){methods.removeEvents();}));}}
function native_scroll_controller(options2){textScroll__content.on('scroll',function(){var pos_px=textScroll__content.scrollTop();var pos=pos_px / Math.floor(heights_delta);options2.onMove(pos,pos_px);});this.move=function(options3){textScroll__content.stop(true).animate({'scrollTop':options3.target*heights_delta},options3.time==undefined?options2.animation_time:options3.time,'easeInOutQuad',function(){if(options3.callback){options3.callback();}});};this.setPosition=function(pos){textScroll__content.stop(true).scrollTop(pos*heights_delta);};this.setScrollSpeed=function(){};this.removeEvents=function(){textScroll__content.off();};return this;}
function setMode(){if(scroll_controller){textScroll__content.removeAttr('style');scroll_controller.removeEvents();}
if(options["native"]){if(textScroll__size.data('hammer')){textScroll__size.data('hammer').destroy();textScroll__size.data('hammer',null).removeAttr('style');}
$item.addClass('native');textScroll__content.css({'right':textScroll__size.width()-$item.width()});scroll_controller=new native_scroll_controller({animation_time:options.animateTime,onMove:function onMove(pos,pos_px){setScrollPosition(pos,pos_px);}});}else{$item.removeClass('native');scroll_controller=new scrollController({events_area:options.eventsArea||$item,animation_time:options.animateTime,scroll_time:options.scrollTime,start_position:options.start_position,keyboard_events:true,autoRemoveEvents:options.autoRemoveEvents,onMove:function onMove(pos){var pos_px=Math.floor(pos*heights_delta);setScrollPosition(pos,pos_px);},animation_callback_before:function animation_callback_before(){pan_data.animated=true;},animation_callback_after:function animation_callback_after(){pan_data.animated=false;},tryChangeEvent:function tryChangeEvent(delta){options.tryChangeEvent(delta);}});}
_resize();}
function setScrollPosition(pos,pos_x){scroll_position=Math.max(0,Math.min(pos,1));scroll_position_px=pos_x;textScroll__track_move.css(get_transition_css(scroll_position*100+'%'));if(!options["native"]){textScroll__content.css(get_transition_css(-scroll_position*100*(max_scroll_position-1)+'%'));}
setTimeout(function(){options.onMove(scroll_position,scroll_position_px);},0);}
function _resize(params){params=$.extend({preserve_position:false},params);textScroll__size_height=textScroll__size.outerHeight();textScroll__content_height=textScroll__content.height();heights_delta=textScroll__size_height-textScroll__content_height;max_scroll_position=Math.max(1,textScroll__size_height / textScroll__content_height);if(max_scroll_position==1){$item.addClass(_this.title+'--no_scroll');}else{$item.removeClass(_this.title+'--no_scroll');}
if(params.fix_position){if(options["native"]){setScrollPosition(textScroll__content.scrollTop()/ heights_delta);}else{scroll_controller.setPosition(-textScroll__content.position().top / heights_delta);}}else if(!params.preserve_position){scroll_controller.setPosition(scroll_position);}
var scroll_speed=options.speed*2 /((max_scroll_position-1)*textScroll__content_height);if(scroll_speed==Infinity){scroll_speed=0;}
scroll_controller.setScrollSpeed(scroll_speed);options.onReinitialise();}
function _scrollToScreen(delta,time){var new_pos=0.01*(scroll_position*100+delta*100 /(max_scroll_position-1));scroll_controller.move({target:new_pos,time:time});}
function stop_animation(){if(pan_data.animated){pan_data.animated=false;scroll_controller.stop();}}
function dragInit(element,parent,text_drag){if(!element.data('hammer')){var calc_controller_position=function calc_controller_position(pos,normalize){pos /=pan_data.drag_height*(1-(text_drag?max_scroll_position:0));if(normalize){pos=Math.max(0,Math.min(1,pos));}
return pos;};element.hammer();element.data('hammer').get('pan').set({direction:Hammer.DIRECTION_VERTICAL,threshold:0});element.data('hammer').get('press').set({time:0});element.on('press panstart panmove panend',function(e){if(events_on&&(!text_drag||options.dragEvents)){if(e.type=='press'){stop_animation();}else if(e.type=='panstart'){stop_animation();pan_data.drag_start_y=parent.position().top;pan_data.drag_height=parent.height();}else if(e.type=='panmove'){var pos_y=calc_controller_position(pan_data.drag_start_y+e.gesture.deltaY,true);scroll_controller.setPosition(pos_y);}else if(e.type=='panend'){var time=Math.abs(400*e.gesture.velocityY);var dist=0.5*e.gesture.velocityY*time;var pos_y=calc_controller_position(pan_data.drag_start_y+e.gesture.deltaY+dist,false);scroll_controller.move({target:pos_y,time:time,easing:'quadEaseOut'});}}});}}
function setAutoresize(){clearInterval(autoresizeInterval);if(options.autoresize){autoresizeInterval=setInterval(function(){_resize();},options.autoresize);}
_resize();}
function setImgresize(){textScroll__size.find('img').off('load.text_scroll').on('load.text_scroll',function(){_resize();});}
var methods={resize:function resize(params){_resize(params);},removeEvents:function removeEvents(){scroll_controller.removeEvents();clearInterval(autoresizeInterval);textScroll__drag.off();textScroll__size.off();textScroll__track.off();textScroll__track.find('.textScroll__page_btn').off();$item.off();listeners.forEach(function(listener){listener.unsubscribe();});},scrollTo:function scrollTo(position,time,callback){scroll_controller.move({target:position,time:time,callback:callback});},setPosition:function setPosition(position){scroll_controller.setPosition(position);},scrollToScreen:function scrollToScreen(delta,time){_scrollToScreen(delta,time);},scrollToElement:function scrollToElement(selector,time,callback,target){var element=$item.find(selector);if(element.length){var target_delta=0;if(target=='bottom'){target_delta=element.outerHeight()-textScroll__content_height;}else if(target=='center'){target_delta=0.5*(element.outerHeight()-textScroll__content_height);}
var position=(element.offset().top-textScroll__size.offset().top+target_delta)/ heights_delta;scroll_controller.move({target:position,time:time,callback:callback});}},getScrollPosition:function getScrollPosition(){return[scroll_position,scroll_position_px];},loadHtml:function loadHtml(html,time,callback){var time=time||0;$item.transitionStop(true).transition({'opacity':0},time,function(){textScroll__size.find('.content').html(html);setImgresize();_resize();scroll_controller.setPosition(0);$item.transition({'opacity':1},time);if(callback){callback();}});},eventsOn:function eventsOn(){events_on=true;scroll_controller.eventsOn();$item.removeClass(_this.title+'--events_off');},eventsOff:function eventsOff(){events_on=false;scroll_controller.eventsOff();$item.addClass(_this.title+'--events_off');},setOptions:function setOptions(new_options){$.extend(options,new_options);if(new_options.dragEvents==true&&!options["native"]){dragInit(textScroll__size,textScroll__content,true);}
if(new_options.autoresize!==undefined){setAutoresize();}
if(new_options["native"]!==undefined){setMode();}}};$item.data(_this.title,methods);init();}});;
page_js.pages["contacts"] = function(page,frame,frame1,frame2){
page.unset=function(){frame.off();};var text_scroll=frame.find('.page__scroll').textScroll({onMove:function onMove(pos){}});text_scroll.eventsOff();var map=new mapsInit({container:frame.find('.map_place'),center:[55.728758,37.730765],zoom:15,zoomButtons:frame.find('.mapsInit__zoom-button')});map.scrollWheelZoom.disable();var marker=new map.marker({position:[55.736271,37.720987],html:'<div class="maps-marker-object"><span>'+_t['addr_1']+'</span></div>'});var marker2=new map.marker({position:[55.879482,37.433489],html:'<div class="maps-marker-object"><span>'+_t['addr_2']+'</span></div>'});var marker3=new map.marker({position:[55.692957,37.427130],html:'<div class="maps-marker-object"><span>'+_t['addr_3']+'</span></div>'});mapUpdateWidth();var events_controller=new eventsController({el:frame.find('.page__events'),max_num:3,num:1,changeEvent:function changeEvent(num,prev_num,delta){if(num==0)return;if(num==1){if(window.innerWidth>1366){map.flyTo([55.727489,37.721554],15);}else{map.flyTo([55.725,37.721554],14);}}
if(num==2){$('.screen_2').addClass("active");$('body').addClass("landing-popup-opened");map.flyTo([55.75531681718604,37.623062084498244],11);$('.page__scroll-help ').removeClass("landing-hidden");}else{$('.screen_2').removeClass("active");$('body').removeClass("landing-popup-opened");$('.page__scroll-help ').addClass("landing-hidden");}
if(num==3)$('body').addClass("hide-help");else $('body').removeClass("hide-help");if(events_controller)events_controller.eventsOff();text_scroll.scrollToElement(".screen_"+num,600,function(){events_controller.eventsOn();});}});frame.find('.page__form-frame').formInit();frame.on('click','.map_element',function(){events_controller.change({target:2});});page.init=function(){mapUpdateWidth();};window.addEventListener('resize',function(){mapUpdateWidth();});function mapUpdateWidth(){$('.screen_left-item').css({'width':$('.screen_1').offset().left*5 / 6});$('.screen_right-item').css({'width':$(window).innerWidth()-$('.screen_1').offset().left-$('.screen_1').innerWidth()});}
};;
