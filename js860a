function get_pathname(){var search=window.location.search.replace(/^\?/,'');return(window.location.pathname+(search?'?'+search:'')+window.location.hash).replace(/#!?/,'/').replace(/(.)\/$/,'$1').replace(/\/{2,}/g,'/');}
function change_url(url,args){if(url&&url.charAt(0)!=='/')console.error('Pjax: missing / in "'+url+'"');if(url!=get_pathname()){args=$.extend({},args);if(args.suppress_load){prev_page=url;}
if(args.click_pos){pjax_click_pos=args.click_pos;}
if(support_pjax){load_content(url,args);history.pushState(url,null,url);}else{window.location.hash='#!'+url;}}}
function replace_url(url){prev_page=url;if(support_pjax){history.replaceState(url,null,url);}else{window.location.hash='#!'+url;}}
function success_load(data,deffered){window.document.title=data.title;$('meta[name=keywords]').attr('content',data.keywords);$('meta[name=description]').attr('content',data.description);options.success(data,deffered);}
function load_content(url,args){if(url&&(url!=prev_page||ajax_first_load)){ajax_first_load=false;prev_page=url;var deffered={};options.beforeSend(url,deffered);if(pages_cache[url]){success_load(pages_cache[url],deffered);}else{if(!args){args={};}
if(!args.get_params){args.get_params={};}
args.get_params.ls=loaded_scripts;$.ajax({type:"POST",cache:false,url:url,data:args.get_params,beforeSend:function beforeSend(request){request.setRequestHeader('X-PJAX','true');},success:function success(data){success_load(data,deffered);if(options.cache){pages_cache[url]=data;}},error:function error(){load_content('/page404');}});}}}
